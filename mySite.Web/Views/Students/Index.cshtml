@model IEnumerable<StudentViewModel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Student list</title>
</head>
<body>
    <h1>Hello</h1>
    <div class="student-content"> 
        @foreach (var student in Model)
        {
            <div class="student">
                @Html.DisplayFor(o => student)
            </div>
        }
    </div>
</body>
</html>
